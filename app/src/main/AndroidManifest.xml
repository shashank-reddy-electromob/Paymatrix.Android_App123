            android:label="@string/title_activity_pay_rentctivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".TransactionHistory"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".UserProfileActivity" />
        <activity android:name=".TranscationDetailsActivity" />
        <activity android:name=".PayRentArchiveActivity" />
        <activity android:name=".AddPropertyActivity" />
        <activity android:name=".UserDashboardActivity" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>